<h1>RegisteredApplications#index</h1>

  <% @registered_applications.each do |app| %>
    <p><%= app.name %></p>
    <p><%= app.url %></p>
    <p><%= app.user.name %></p>
    <%= link_to "Display info", registered_application_path(app) %>
    <%= link_to "Edit" , edit_registered_application_path(app) %>
    <%= link_to "Delete", registered_application_path(app), method: :delete %>
  <% end %>
